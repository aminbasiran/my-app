{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/react-in-jsx-scope": "off",
    "react/prop-types": ["error", {
      "skipUndeclared": false
    }]
    
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
